{
  "workbench.iconTheme": "vscode-icons",
  "editor.tabSize": 2,
  "editor.rulers": [80],
  "editor.fontSize": 12,
  "files.trimTrailingWhitespace": true,
  "telemetry.enableTelemetry": false,
  "telemetry.enableCrashReporter": false,
  "gitlens.advanced.telemetry.enabled": false,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/.DS_Store": true,
    "**/.vscode": true,
    "**/node_modules": false,
    "**/.bundle": true,
    "**/.yardoc": true,
    "**/tmp": true,
    "**/deps": true,
    "**/_build": true,
    "_universe": true,
    ".expo": true,
    ".expo-source": true,
    "node_modules": true,
    ".travis": true,
    "package-lock.json": true,
    "yarn.lock": true,
  },
  "eslint.options": { "configFile": ".eslintrc" },
  "editor.tabCompletion": true,
  "editor.minimap.enabled": false,
  "editor.hideCursorInOverviewRuler": true,
  "editor.renderWhitespace": "boundary",
  "workbench.sideBar.location": "left",
  "workbench.statusBar.visible": true,
  "vsicons.dontShowNewVersionMessage": true,
  "files.insertFinalNewline": true,
  "window.zoomLevel": 0,
  "workbench.activityBar.visible": true,
  "explorer.openEditors.visible": 0,
  "files.associations": {
    "*.html.ui": "ruby"
  },
  "workbench.colorTheme": "Monokai",
  "workbench.editor.enablePreview": false,
  "extensions.autoUpdate": true,
  "window.title": "${activeEditorMedium}",
  "typescript.check.npmIsInstalled": false,
  "workbench.startupEditor": "newUntitledFile",
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmDelete": false,
  "ActiveFileInStatusBar.fullpath": false,
  "ActiveFileInStatusBar.revealFile": true,
  "cryptoticker.cryptoSymbols": ["BTC", "LTC"],
  "cryptoticker.cryptoCurrency": "USD",
  "cryptoticker.cryptoExchange": "Coinbase",
  "zenMode.hideStatusBar": false,
  "gitlens.advanced.messages": {
    "suppressCommitHasNoPreviousCommitWarning": false,
    "suppressCommitNotFoundWarning": false,
    "suppressFileNotUnderSourceControlWarning": false,
    "suppressGitVersionWarning": false,
    "suppressLineUncommittedWarning": false,
    "suppressNoRepositoryWarning": false,
    "suppressResultsExplorerNotice": true,
    "suppressShowKeyBindingsNotice": true,
    "suppressUpdateNotice": false,
    "suppressWelcomeNotice": true
  },
  "gitlens.keymap": "alternate",
  "workbench.statusBar.feedback.visible": false,
  "gitlens.historyExplorer.enabled": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "ruby.intellisense": "rubyLocate",
  "githubPullRequests.hosts": [
    {
      "host": "https://github.com",
      "username": "oauth",
      "token": "system"
    }
  ],
}
